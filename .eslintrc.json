{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "node/no-unsupported-features/es-syntax": ["error", { "ignores": ["modules"] }]
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  }
  